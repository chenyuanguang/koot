{
    "name": "koot-test-standard",
    "version": "0.3.0",
    "description": "Koot.js 模板项目",
    "author": {
        "name": "Diablohu",
        "email": "diablohudream@gmail.com",
        "url": "http://diablohu.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cmux/koot-boilerplate"
    },
    "license": "Apache-2.0",
    "scripts": {
        "start": "koot-start",
        "start:0.8:full": "koot-start --config koot.config.0.8-full.js",
        "start:server": "koot-start --no-build",
        "build": "koot-build",
        "build:client": "koot-build -c",
        "build:server": "koot-build -s",
        "build:spa": "koot-build --type spa --dest dist-spa",
        "build:spa:use-config-file": "koot-build --config koot.config.spa.js",
        "dev": "koot-dev",
        "dev:0.8:full": "koot-dev --config koot.config.0.8-full.js",
        "dev:client": "koot-dev -c",
        "dev:server": "koot-dev -s",
        "dev:global-pm2": "koot-dev -g",
        "dev:no-open": "koot-dev --no-open",
        "dev:spa": "koot-dev --type spa --dest dist-spa",
        "dev:spa:no-open": "koot-dev --type spa --dest dist-spa --no-open",
        "analyze": "koot-analyze",
        "eslint": "eslint \"src/**/*.+(js|jsx)\"",
        "test:server": "npm run build:server && node dist\\server\\index.js",
        "koot-buildtest-isomorphic-build": "koot-build --env prod --koot-test",
        "koot-buildtest-isomorphic-start-server": "koot-start --no-build --koot-test",
        "koot-buildtest-isomorphic-start-server-custom-port": "koot-start --no-build --port 8316 --koot-test",
        "koot-buildtest-isomorphic-dev": "koot-dev --no-open --koot-test",
        "koot-buildtest-spa-build": "koot-build --type react-spa --dest dist-spa-test --koot-test",
        "koot-buildtest-isomorphic-start-i18n_use_router": "koot-start --koot-test --config koot.config.i18n-use-router.js",
        "koot-buildtest-isomorphic-dev-i18n_use_router": "koot-dev --no-open --koot-test --config koot.config.i18n-use-router.js",
        "koot-buildtest-isomorphic-start-config_old_0.6": "koot-start --koot-test --config koot.config.old-0.6.js",
        "koot-buildtest-isomorphic-dev-config_old_0.6": "koot-dev --no-open --koot-test --config koot.config.old-0.6.js"
    },
    "dependencies": {
        "autoprefixer": "9.4.7",
        "koot": "file:../../../packages/koot",
        "normalize.css": "8.0.0",
        "cssnano": "4.1.7",
        "svg-url-loader": "2.3.2",
        "axios": "0.18.0",
        "koot-webpack": "file:../../../packages/koot-webpack"
